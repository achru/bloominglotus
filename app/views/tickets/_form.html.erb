<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :entries %>
    <%= f.input :valid_until %>
    <%= f.input :unlimited %>
    <%= f.association :student %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
